---
layout: null
---
[
  {% for post in site.posts %}{
    "id": {{ forloop.index }},
    "title": {{ post.title | jsonify }},
    "date": "{{ post.date | date_to_xmlschema }}",
    "url": "{{ site.url }}{{ post.url }}",
    "excerpt": "{{ post.excerpt }}",
    "tags": {{ post.tags | jsonify }}
  }{% unless forloop.last %},{% endunless %}{% endfor %}
]
